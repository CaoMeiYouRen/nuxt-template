{
  "name": "nuxt-template",
  "version": "0.1.0",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "cross-env NODE_ENV=production eslint --fix --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "cross-env NODE_ENV=production stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore --syntax scss --custom-syntax postcss-html --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "cross-env NODE_ENV=production eslint --fix --ext \".js,.ts,.vue\"",
    "*.{css,scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "core-js": "^3.20.3",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@vue/eslint-config-typescript": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-cmyr": "^1.1.14",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^7.20.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.2",
    "stylelint": "^13.13.1",
    "stylelint-config-cmyr": "^0.2.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "^4.5.5"
  }
}